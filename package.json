{
  "name": "@radoslawgrochowski/eslint-config",
  "version": "2.2.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/radoslawgrochowski/eslint-config.git"
  },
  "main": "eslint.config.mjs",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "configurations/*.mjs",
    "eslint.config.mjs",
    "prettier.config.mjs"
  ],
  "scripts": {
    "format": "prettier --ignore-path .prettierignore --ignore-path .gitignore **/*.{json,md} --list-different",
    "format:fix": "pnpm format --write",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "prepare": "husky",
    "release": "semantic-release",
    "type-check": "tsc"
  },
  "exports": {
    "./*": "./configurations/*.mjs",
    ".": "./eslint.config.mjs"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "radoslawgrochowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/radoslawgrochowski/eslint-config/issues"
  },
  "homepage": "https://github.com/radoslawgrochowski/eslint-config#readme",
  "peerDependencies": {
    "eslint": ">= 9 < 10"
  },
  "dependencies": {
    "@eslint/js": "^9.12.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "typescript-eslint": "^8.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "20.4.0",
    "@commitlint/config-conventional": "20.4.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@tsconfig/node-lts": "22.0.4",
    "eslint": "9.39.2",
    "husky": "9.1.7",
    "semantic-release": "25.0.3",
    "typescript": "5.9.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
